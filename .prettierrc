{
  "plugins": ["prettier-plugin-packagejson"],
  "semi": false,
  "singleQuote": true,
  "useTabs": true,
  "importOrder": [
    "(^react$|^react-dom(.*)$|^react-router-dom$)",
    "<THIRD_PARTY_MODULES>",
    "(^~(.*)$)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
